{
  "name": "bienesraices",
  "version": "1.0.0",
  "description": "Mi primer proyecto con node",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "css": "postcss public/css/tailwindcss.css -o public/css/app.css --watch",
    "js": "webpack --watch",
    "dev": "concurrently \"npm run css\" \"npm run js\" ",
    "db:importar": "node ./seed/seeder.js -i",
    "db:eliminar": "node ./seed/seeder.js -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perseo-2025/bienesraices-node.git"
  },
  "keywords": [
    "MVC",
    "Pug",
    "Tailwind",
    "Mysql",
    "Sequelize"
  ],
  "author": "Rey Perseo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Perseo-2025/bienesraices-node/issues"
  },
  "homepage": "https://github.com/Perseo-2025/bienesraices-node#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "dropzone": "^5.9.3",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "flowbite": "^2.5.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
